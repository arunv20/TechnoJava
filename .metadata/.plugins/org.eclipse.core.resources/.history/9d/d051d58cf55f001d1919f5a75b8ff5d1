package com.te.restaurant.module1;

import java.util.Scanner;

public class RestaurantItems {
	
		static String [] fooditems= new String[] {"briyani","Phulka","roti","b.Masala"};
		 static int[] rate = new int [] {120,50,20,50};	
		int [] qt = new int [0];
	public static void displayItems(){
			System.out.println(" "+"items"+"\t\t\tprice");
			
			for(int i=0;i<4;i++) {
				System.out.println((i+1)+fooditems[i]+"\t\t\t"+ rate[i]);
			}
	}
	static Scanner scanner = new Scanner(System.in);
	public static void order(int[] order) {
		for(int i=0;i<4;i++) {
			System.out.println("Enter your order number");
			order[i+1]=scanner.nextInt();
			
		}
		
	}
	public static void main(String[] args) {
		RestaurantItems.displayItems();
		
	}
		
	
	

}